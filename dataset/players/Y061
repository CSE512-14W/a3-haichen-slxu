		pp_params.bgcolor = "#ffffff";
		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontentd8a8ecc78cdb4905a44bf2c28abec87b";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontentd8a8ecc78cdb4905a44bf2c28abec87b", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

</div>




