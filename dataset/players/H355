		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent7344755fba1f4695a1ad29189343481e";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent7344755fba1f4695a1ad29189343481e", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

</div>




