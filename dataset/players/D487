		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent24ed5ca902154e94a2a68a226e93931f";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent24ed5ca902154e94a2a68a226e93931f", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

