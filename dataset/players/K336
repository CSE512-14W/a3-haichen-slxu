		pp_params.scale = "noscale";
		pp_params.bgcolor = "#ffffff";
		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent7f7193a1707249b3aec0c700dc92aefb";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent7f7193a1707249b3aec0c700dc92aefb", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

