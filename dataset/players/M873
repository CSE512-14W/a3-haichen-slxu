		pp_params.scale = "noscale";
		pp_params.bgcolor = "#ffffff";
		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent5dd9205ef6734250974110989bcac234";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent5dd9205ef6734250974110989bcac234", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

