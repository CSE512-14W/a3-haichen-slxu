		pp_params.bgcolor = "#ffffff";
		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent02994496b49b4d21981c83640f360b8b";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent02994496b49b4d21981c83640f360b8b", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

</div>




